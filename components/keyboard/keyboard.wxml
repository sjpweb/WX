        <button class="key_btn" bindtap="onKeyBoardPress" data-text="鄂">鄂</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="湘">湘</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="粤">粤</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="桂">桂</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="渝">渝</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="川">川</button>

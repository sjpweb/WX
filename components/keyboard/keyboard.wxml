        <button class="key_btn" bindtap="onKeyBoardPress" data-text="吉">吉</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="黑">黑</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="沪">沪</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="苏">苏</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="浙">浙</button>

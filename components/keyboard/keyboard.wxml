        <button class="key_btn" bindtap="onKeyBoardPress" data-text="琼">琼</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="新">新</button>
        <button class="key_btn change normalWidth" bindtap="changeKeyBoard">ABC</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="宁">宁</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="港">港</button>

<!--components/keyboard.wxml-->
<van-popup
    show="{{ show }}"
    position="bottom"
    bind:close="onClose"
    overlay="{{false}}"
    custom-style="background:#BBC2CA;font-size:40rpx;"
  >
    <text class="clearInput" bindtap="clearInput">清空</text>
    <text class="complete" bindtap="inputComplete">完成</text>
    <view class="keyboard">
      <view wx:if="{{showCnKeyBoard}}" class="keyboard_content">
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="京">京</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="津">津</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="冀">冀</button>
        <button class="key_btn" bindtap="onKeyBoardPress" data-text="鲁">鲁</button>
